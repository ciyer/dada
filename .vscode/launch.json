{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
      
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'dada' on hello-world",
            "cargo": {
                "args": [
                    "build",
                    "--bin=dada",
                    "--package=dada"
                ],
                "filter": {
                    "name": "dada",
                    "kind": "bin"
                }
            },
            "args": [
                "compile",
                "tests/parser/fn_body.dada"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            // Used for testing the extension with a local build of the LSP server.
            "name": "Run Extension (Debug Build)",
            "type": "extensionHost",
            "request": "launch",
            "runtimeExecutable": "${execPath}",
            "args": [
              "--disable-extensions",
              "--extensionDevelopmentPath=${workspaceFolder}/editors/code"
            ],
            "outFiles": [
              "${workspaceFolder}/editors/code/out/**/*.js"
            ],
            "preLaunchTask": "Build Dada and Extension",
            "skipFiles": [
              "<node_internals>/**/*.js"
            ],
            "env": {
              "__DADA_LSP_SERVER_DEBUG": "${workspaceFolder}/target/debug/dada"
            }
          },
    ]
}